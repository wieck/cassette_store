PROGRAM "ANNUITY"
10:INPUT "N:",A:END 
20:INPUT "I:",S:END 
30:INPUT "PV:",D:END 
40:INPUT "PMT:",F:END 
50:INPUT "FW:",G:END 
300:GOSUB 900
310:D=(G+F*E)/W
320:PRINT "PV=",D:END 
400:GOSUB 900
410:F=(D*W-G)/E
420:PRINT "PMT=",F:END 
500:GOSUB 900
510:G=D*W-F*E
520:PRINT "FW=",G:END 
900:Q=1+S/100
901:W=Q^A
902:E=(W-1)/(Q-1)
903:RETURN 
